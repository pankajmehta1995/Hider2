<%// JSON.stringify(resultset) %>
<!-- END PAGE HEADER--> 
<!-- BEGIN PAGE CONTENT-->

<div class="row profile">
  <div class="col-md-12"> 
    <!--BEGIN TABS-->
    <div class="tabbable tabbable-custom tabbable-full-width">
      <ul class="nav nav-tabs">
        <li class="active"> <a href="#tab_1_1" data-toggle="tab">Edit User</a> </li>
      </ul>
      <div id="message" style="margin-top: -25px; margin-left: 284px; position:absolute;"> 
		  <font color='green'> <%= messages.successmsg; %> </font> 
		  <font color='red'> <%= messages.errormsg; %> </font> 
	  </div>
      <div class="tab-content">
        <div class="tab-pane active" id="tab_1_1">
          <div class="row">
            <div class="col-md-9">
            <div class="row"></div>
            <!--end row-->
            <div class="tab-pane active" id="tab1"></div>
            <form name="frm" id="editUser" action="editUser" method="post">
              
              <input type="hidden" name="user_id" value="<%= resultset['id']; %>">
              
              <div class="form-group flare">
                <label class="control-label col-md-4"> Username<span class="required"> * </span> </label>
                <div class="col-md-5">
                  <input class="form-control"  name="user_name"  id="user_name"  disabled="disabled" type="text" value="<%= ((resultset['username']!=null && resultset['username']!='') ? resultset['username'] : ''); %>"
                </div>
              </div>
              <div class="form-group flare">
                <label class="control-label col-md-4">First Name<span class="required"> * </span> </label>
                <div class="col-md-5">
                  <input class="form-control"  name="first_name" id="first_name" type="text" value="<%= ((resultset['first_name']!=null && resultset['first_name']!='') ? resultset['first_name'] : ''); %>"/>
                </div>
              </div>
              <div class="form-group flare">
                <label class="control-label col-md-4">Last Name<span class="required"> * </span> </label>
                <div class="col-md-5">
                  <input class="form-control"  name="last_name" id="last_name" type="text" value="<%= ((resultset['last_name']!=null && resultset['last_name']!='') ? resultset['last_name'] : ''); %>"/>
                </div>
              </div>
              <div class="form-group flare">
                <label class="control-label col-md-4"> Email<span class="required"> * </span> </label>
                <div class="col-md-5">
                  <input class="form-control"  name="email" disabled="disabled" id="email" type="text" value="<%= ((resultset['email']!=null && resultset['email']!='') ? resultset['email'] : ''); %>"/>
                </div>
              </div>
               <div class="form-group flare">
                <label class="control-label col-md-4"> Gender<span class="required"> * </span> </label>
                <div class="col-md-5">
                  <input class="form-control"  name="gender"  id="gender" type="text" value="<%= ((resultset['gender']!=null && resultset['gender']!='') ? resultset['gender'] : ''); %>"/>
                </div>
              </div>
              <div class="form-group">
                <label class="control-label col-md-4">&nbsp; </label>
                <div class="col-md-8">
                  <button type="submit" value="Save" class="btn theme-btn voilet green pull-left" onclick="return validation_user();">Save</button>
                  <a href="manageUser"  class="btn blue pull-left margd">Cancel</a> </div>
              </div>
              </div>
             
              
            </form>
          </div>
        </div>
      </div>
    </div>
    <!--tab_1_2--> 
    
  </div>
</div>

<!--END TABS-->
</div>
<!-- END PAGE CONTENT--> 

