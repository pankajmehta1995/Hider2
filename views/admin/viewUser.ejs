<!-- BEGIN PAGE CONTENT-->
<div class="row profile">
  <div class="col-md-12"> 
    <!--BEGIN TABS-->
    <div class="tabbable tabbable-custom tabbable-full-width">
      <ul class="nav nav-tabs">
        <li class="active"> <a href="#tab_1_1" data-toggle="tab">View User</a> </li>
      </ul>
      <div class="tab-content">
        <div class="tab-pane active" id="tab_1_1">
          <div class="row">
            <div class="col-md-12">
              <div class="row"> 
                <!--end col-md-8--> 
                <!--end col-md-4--> 
              </div>
              <!--end row-->
              <div class="tab-pane active" id="tab1">
                <div class="form-group detra">
                  <label class="control-label col-md-4">User Name :</label>
                  <div class="col-md-8"> <span class="detail_for_name">
                    <%= ((resultset['username']!=null && resultset['username']!='') ? resultset['username'] : 'N/A'); %>
                    </span> </div>
                </div>
                <div class="form-group detra">
                  <label class="control-label col-md-4">Name :</label>
                  <div class="col-md-8"> <span class="detail_for_name">
                    <%= ((resultset['first_name']!=null && resultset['first_name']!='') ? resultset['first_name'] : '')+' '+(resultset['last_name']!=null ? resultset['last_name'] : 'N/A'); %>
                    </span> </div>
                </div>
                <div class="form-group detra">
                  <label class="control-label col-md-4">Email Address :</label>
                  <div class="col-md-8"> <span class="detail_for_name">
                    <!--?php  echo !empty($resultset['email']) ? ($resultset['email']) : "N/A"; ?-->
                     <%= resultset['email']; %>
                    </span> </div>
                </div>
                
                 <div class="form-group detra">
                  <label class="control-label col-md-4">Gender :</label>
                  <div class="col-md-8"> <span class="detail_for_name">
					 <%= ((resultset['gender']!=null && resultset['gender']!='') ? resultset['gender'] : 'N/A'); %>	
                    </span> </div>
                </div>
                <div class="form-group detra">
                  <label class="control-label col-md-4">Date of birth :</label>
                  <div class="col-md-8"> <span class="detail_for_name">
                    <!--?php  echo !empty($resultset['time']) ? date('d-M-Y',$resultset['time']) : ""; ?-->
                     <%= resultset['date_of_birth']; %>
                    </span> </div>
                </div>
                
                <div class="form-group detra">
                  <label class="control-label col-md-4">Joining date :</label>
                  <div class="col-md-8"> <span class="detail_for_name">
                    <!--?php  echo !empty($resultset['time']) ? date('d-M-Y',$resultset['time']) : ""; ?-->
                    <%= resultset['created']; %>
                    </span> </div>
                </div>
                <div class="form-group">
                <label class="control-label col-md-4">&nbsp; </label>
                <div class="col-md-8">
                  <a href="manageUser"  class="btn blue pull-left margd">Cancel</a> </div>
              </div>
              </div>
            </div>
          </div>
          <!--tab_1_2--> 
        </div>
      </div>
      <!--END TABS--> 
    </div>
  </div>
  <!-- END PAGE CONTENT--> 
